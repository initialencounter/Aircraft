{
  "name": "aircraft",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*",
    "electron",
    "bindings/node"
  ],
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier --write . ",
    "dep": "yakumo upgrade",
    "dev": "vite",
    "dev:el": "cd electron && yarn start",
    "build:el": "cd electron && yarn build",
    "build": "vue-tsc --noEmit && vite build",
    "dev:t": "tauri dev",
    "build:t": "tauri build",
    "build:v": "cd packages/validators && yarn build",
    "build:w": "cd packages/wxt && yarn build && yarn build:firefox",
    "preview": "vite preview",
    "tauri": "tauri",
    "set-proxy": "npm config set registry https://registry.npmmirror.com",
    "unset-proxy": "npm config delete registry"
  },
  "devDependencies": {
    "@maikolib/vite-plugin-yaml": "^1.1.1",
    "@tauri-apps/cli": "^2.9.6",
    "@typescript-eslint/eslint-plugin": "^8.50.2-alpha.6",
    "@typescript-eslint/parser": "^8.50.2-alpha.7",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue": "^3.6.0-beta.1",
    "vue-tsc": "^3.2.1",
    "yakumo": "^1.0.0",
    "yakumo-upgrade": "^0.3.6"
  },
  "dependencies": {
    "@aircraft/renderer": "workspace:*"
  },
  "packageManager": "yarn@4.12.0"
}
